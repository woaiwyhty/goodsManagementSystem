doctype html
html(lang='en-US')
    head
        meta(charset='UTF-8')
        meta(name='viewport', content='width=device-width, initial-scale=1.0')
        title Kanrisha - A Premium HTML5 Responsive Admin Template
        //if lt IE 9
            script(src='http//css3-mediaqueries-js.googlecode.com/svn/trunk/css3-mediaqueries.js')
            script(src='http//html5shiv.googlecode.com/svn/trunk/html5.js')
            script(src='Javascript/Flot/excanvas.js')
        // The Fonts
        link(href='http//fonts.googleapis.com/css?family=Oswald|Droid+Sans:400,700', rel='stylesheet')
        // The Main CSS File
        link(rel='stylesheet', href='CSS/style.css')
        // jQuery
        script(src='Javascript/jQuery/jquery-1.7.2.min.js')
        // Flot
        script(src='Javascript/Flot/jquery.flot.js')
        script(src='Javascript/Flot/jquery.flot.resize.js')
        script(src='Javascript/Flot/jquery.flot.pie.js')
        // DataTables
        script(src='Javascript/DataTables/jquery.dataTables.min.js')
        // ColResizable
        script(src='Javascript/ColResizable/colResizable-1.3.js')
        // jQuryUI
        script(src='Javascript/jQueryUI/jquery-ui-1.8.21.min.js')
        // Uniform
        script(src='Javascript/Uniform/jquery.uniform.js')
        // Tipsy
        script(src='Javascript/Tipsy/jquery.tipsy.js')
        // Elastic
        script(src='Javascript/Elastic/jquery.elastic.js')
        // ColorPicker
        script(src='Javascript/ColorPicker/colorpicker.js')
        // SuperTextarea
        script(src='Javascript/SuperTextarea/jquery.supertextarea.min.js')
        // UISpinner
        script(src='Javascript/UISpinner/ui.spinner.js')
        // MaskedInput
        script(src='Javascript/MaskedInput/jquery.maskedinput-1.3.js')
        // ClEditor
        script(src='Javascript/ClEditor/jquery.cleditor.js')
        // Full Calendar
        script(src='Javascript/FullCalendar/fullcalendar.js')
        // Color Box
        script(src='Javascript/ColorBox/jquery.colorbox.js')
        // Kanrisha Script
        script(src='Javascript/kanrisha.js')
        meta(http-equiv='Content-Type', content='text/html; charset=utf-8')
    body
        // Change Pattern
        .changePattern
            span#pattern1
            span#pattern2
            span#pattern3
            span#pattern4
            span#pattern5
            span#pattern6
        // Top Panel
        .top_panel
            .wrapper
                .user
                    img.user_avatar(src='Images/user_avatar.png', alt='user_avatar')
                    span.label John Alex
                    // Top Tooltip
                    .top_tooltip
                        div
                            ul.user_options
                                li.i_16_profile
                                    a(href='#', title='Profile')
                                li.i_16_tasks
                                    a(href='#', title='Tasks')
                                li.i_16_notes
                                    a(href='#', title='Notes')
                                li.i_16_options
                                    a(href='#', title='Options')
                                li.i_16_logout
                                    a(href='#', title='Log-Out')
                .top_links
                    ul
                        li.i_22_search.search_icon
                            .top_tooltip.right_direction
                                div
                                    form.top_search_form
                                        input.top_search_input(type='text')
                                        input.top_search_submit(type='submit', value='')
                        li.i_22_settings
                            a(href='#', title='Settings')
                                span.label Settings
                        li.i_22_upload
                            a(href='#', title='Upload')
                                span.label Upload
                            // Drop Menu
                            ul.drop_menu.menu_with_icons.right_direction
                                li
                                    a.i_16_add(href='#', title='New Flie')
                                        span.label New File
                                li
                                    a.i_16_progress(href='#', title='2 Files Left')
                                        span.label Files Left
                                        span.small_count 2
                                li
                                    a.i_16_files(href='#', title='Browse Files')
                                        span.label Browse Files
                        li.i_22_inbox.top_inbox
                            a(href='#', title='Inbox')
                                span.label.lasCount Inbox
                                span.small_count 3
                        li.i_22_pages
                            a(href='#', title='Pages')
                                span.label Pages
                            // Drop Menu
                            ul.drop_menu.menu_without_icons
                                li
                                    a(title='403 Page', href='403.html')
                                        span.label 403 Forbidden
                                li
                                    a(href='404.html', title='404 Page')
                                        span.label 404 Not Found
        header.main_header
            .wrapper
                .logo
                    a(href='#', title='Kanrisha Home')
                        img(src='Images/kanrisha_logo.png', alt='kanrisha_logo')
                nav.top_buttons
                    ul
                        li.big_button
                            .out_border
                                .button_wrapper
                                    .in_border
                                        a.the_button(href='#', title='Analytics')
                                            span.i_32_statistic
                        li.big_button
                            .big_count
                                span 7
                            .out_border
                                .button_wrapper
                                    .in_border
                                        a.the_button(href='#', title='Support')
                                            span.i_32_support
                        li.big_button
                            .out_border
                                .button_wrapper
                                    .in_border
                                        a.the_button(href='#', title='Delivery')
                                            span.i_32_delivery
                        li.big_button
                            .out_border
                                .button_wrapper
                                    .in_border
                                        a.the_button(href='#', title='Earning')
                                            span.i_32_dollar
        .wrapper.small_menu
            ul.menu_small_buttons
                li
                    a.i_22_dashboard.smActive(title='General Info', href='index.html')
                li
                    a.i_22_inbox(title='Your Messages', href='inbox.html')
                li
                    a.i_22_charts(title='Visual Data', href='charts.html')
                li
                    a.i_22_ui(title='Kit elements', href='ui.html')
                li
                    a.i_22_tables(title='Some Rows', href='tables.html')
                li
                    a.i_22_forms(title='Some Fields', href='forms.html')
        .wrapper.contents_wrapper
            aside.sidebar
                ul.tab_nav
                    li.active_tab.i_32_dashboard
                        a(href='index.html', title='General Info')
                            span.tab_label Dashboard
                            span.tab_info General Info
                    li.i_32_inbox
                        a(href='inbox.html', title='Your Messages')
                            span.tab_label Inbox
                            span.tab_info Your Messages
                    li.i_32_charts
                        a(href='charts.html', title='Visual Data')
                            span.tab_label Charts
                            span.tab_info Visual Data
                    li.i_32_ui
                        a(href='ui.html', title='Kit elements')
                            span.tab_label UI
                            span.tab_info Kit elements
                    li.i_32_tables
                        a(href='tables.html', title='Some Rows')
                            span.tab_label Tables
                            span.tab_info Some Rows
                    li.i_32_forms
                        a(href='forms.html', title='Some Fields')
                            span.tab_label Forms
                            span.tab_info Some Fields
            .contents
                .grid_wrapper
                    .g_6.contents_header
                        h3.i_16_dashboard.tab_label Dashboard
                        div
                            span.label General Informations and Resume
                    .g_6.contents_options
                        .simple_buttons
                            .bwIcon.i_16_help Help
                        .simple_buttons
                            .bwIcon.i_16_settings Settings
                            .buttons_arrow
                                // Drop Menu
                                ul.drop_menu.menu_with_icons.right_direction
                                    li
                                        a.i_16_add(href='#', title='New Flie')
                                            span.label New File
                                    li
                                        a.i_16_progress(href='#', title='2 Files Left')
                                            span.label Files Left
                                            span.small_count 2
                                    li
                                        a.i_16_files(href='#', title='Browse Files')
                                            span.label Browse Files
                    .g_12.separator
                        span
                    // Quick Statistics
                    .g_3.quick_stats
                        .big_stats.visitor_stats
                            | 9827
                        h5.stats_info New Visitor
                    .g_3.quick_stats
                        .big_stats.tickets_stats
                            | 23
                        h5.stats_info Open Tickets
                    .g_3.quick_stats
                        .big_stats.users_stats
                            | 982
                        h5.stats_info Users
                    .g_3.quick_stats
                        .big_stats.orders_stats
                            | 2045
                        h5.stats_info Orders
                    .g_12.separator.under_stat
                        span
                    // Charts
                    .g_12
                        .widget_header
                            h4.widget_header_title.wwIcon.i_16_charts Charts
                        .widget_contents
                            .charts
                    // Separator
                    .g_12.separator
                        span
                    // Chats
                    .g_6
                        .widget_header
                            h4.widget_header_title.wwIcon.i_16_chats Chats
                        .widget_contents.noPadding
                            .line_grid
                                .g_2.g_2M
                                    img.avatar(src='Images/Avatar/avatar1.png', alt='avatar')
                                .g_10.g_10M
                                    .message
                                        span.label.lwParagraph
                                            | Lorem ipsum dolor sit amet, consectetur adipiscing elit. Mauris quis urna dui. Vestibulum adipiscing venenatis elementum.
                            .line_grid
                                .g_10.g_10M
                                    .message
                                        span.label.lwParagraph
                                            | Lorem ipsum dolor sit amet, consectetur adipiscing elit. Mauris quis urna dui. Vestibulum adipiscing venenatis elementum.
                                .g_2.g_2M
                                    img.avatar.aR(src='Images/Avatar/avatar3.png', alt='avatar')
                            .line_grid
                                .g_2.g_2M
                                    img.avatar(src='Images/Avatar/avatar1.png', alt='avatar')
                                .g_10.g_10M
                                    .message
                                        span.label.lwParagraph
                                            | Lorem ipsum dolor sit amet, consectetur adipiscing elit. Mauris quis urna dui. Vestibulum adipiscing venenatis elementum.
                            .line_grid
                                .g_2.g_2M
                                    img.avatar(src='Images/Avatar/avatar2.png', alt='avatar')
                                .g_10.g_10M
                                    textarea.mpReply.elastic.simple_field(placeholder='Type your Message and press Enter ..')
                                    .field_notice This Textarea is Elastic :)
                    // Pie Charts
                    .g_6
                        .widget_header
                            h4.widget_header_title.wwIcon.i_16_pie Pie Charts
                        .widget_contents
                            .pie_charts
                    .g_6
                        .widget_header
                            h4.widget_header_title.wwIcon.i_16_add Upload New
                        .widget_contents.noPadding
                            .line_grid
                                .g_12
                                    input.simple_form(type='file')
                                    .field_notice Max Size: 20Mb
                            .line_grid
                                .g_12
                                    input.simple_buttons(type='submit', value='Upload ..')
                    .g_12
                        .widget_header
                            h4.widget_header_title.wwIcon.i_16_add New Article
                        .widget_contents.noPadding
                            .line_grid
                                .g_12
                                    textarea.simple_field.wysiwyg
                                    .field_notice What You See Is What You Get ;)
                    // Separator
                    .g_12.separator
                        span
                    // Calendar
                    .g_12
                        .widget_header.wwOptions
                            h4.widget_header_title.wwIcon.i_16_calendar Calendar
                            .w_Options.i_16_settings
                                // Drop Menu
                                ul.drop_menu.menu_with_icons.right_direction
                                    li
                                        a.i_16_add(href='#', title='New Flie')
                                            span.label New File
                                    li
                                        a.i_16_progress(href='#', title='2 Files Left')
                                            span.label Files Left
                                            span.small_count 2
                                    li
                                        a.i_16_files(href='#', title='Browse Files')
                                            span.label Browse Files
                        .widget_contents.noPadding
                            .aCalendar.autoPadding
        div(style='display:none')
            script(src='http//v7.cnzz.com/stat.php?id=155540&web_id=155540', language='JavaScript', charset='gb2312')
